
{

    "manifest_version": 3,
    "name":"Word Search",
    "version":"1.0.0",
    "description": "Save word search",

    "action": {
        "default_icon": {              
          "16": "images/logo_32.png",   
          "24": "images/logo_32.png",   
          "32": "images/logo_32.png"    
        },
        "default_title": "Save word search",  
        "default_popup": "popup.html"  
    },
    

    "icons":{ 
            "16": "images/256_logo.png",
            "48": "images/256_logo.png",
            "128": "images/256_logo.png" 
    },

    "background": {
        "service_worker": "background.js"
    },
    "permissions": [
        
        "activeTab",
        "tabs",
        "notifications"
     
    
    ],
    "homepage_url": "http://localhost:8000",
    "host_permissions": ["https://twitter.com/home","https://www.google.com/*"]
 

}